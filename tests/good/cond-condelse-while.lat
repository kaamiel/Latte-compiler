int main() {

    Cond1();
    Cond2();
    Cond3();
    CondElse1();
    CondElse2();
    CondElse3();
    loop1();

    return 0;
}

void Cond1() {
    int x = -1;
    if (x > 0) {
        x = 100;
        printInt(x);
    }

    printInt(x);
}

void Cond2() {
    int x = -1;
    if (x < 0 || readInt() > 0) {
        x = 200;
        printInt(x);
    }

    printInt(x);
}

void Cond3() {
    int x = 1;
    if (x > 0 && x - 1 > -1 && 2 * x > x) {
        x = 300;
        printInt(x);
    }

    printInt(x);
}

void CondElse1() {
    int x = -1;
    if (x > 0 && readInt() > 0) {
        x = 100;
        printInt(x);
    } else {
        x = 400;
        printInt(x);
    }

    printInt(x);
}

void CondElse2() {
    int x = -1;
    if (x < 0 || readInt() > 0) {
        x = 500;
        printInt(x);
    } else {
        x = 200;
        printInt(x);
    }

    printInt(x);
}

void CondElse3() {
    int x = 1;
    if (x > 0 && x - 1 > -1 && 2 * x > x) {
        x = 600;
        printInt(x);
    } else {
        x = 200;
        printInt(x);
    }

    printInt(x);
}

void loop1() {
    int x = -1, y = -1;
    while (y > 0 && readInt() > 0) {
        x = -100;
        printInt(x);
    }
    printInt(x);
}
